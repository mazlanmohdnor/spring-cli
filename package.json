{
  "name": "spring-boot-cli-generator",
  "version": "1.0.1-alpha-6",
  "description": "CLI tool for generating boilerplate code for Spring Boot applications.",
  "main": "dist/index.js",
  "bin": {
    "spring": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/templates/* dist/commands/",
    "build": "npm run clean && tsc && npm run copy-files"
  },
  "keywords": ["spring", "boot", "cli", "generator"],
  "author": "Mazlan",
  "license": "ISC",
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.5.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "figlet": "^1.6.0"
  }
}
